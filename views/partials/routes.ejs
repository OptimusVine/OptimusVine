<!DOCTYPE html>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.10/angular-ui-router.js"></script>
    <script src="/javascripts/angularApp.js"></script>

 <script type="text/ng-template" id="/routes.html">
 <div>

 	<div class="page-header" style="margin-left:30px">  
 		<div>
 			<table class="table table-hover">
 				<tr>
 					<th>Name</th>
 					<th>Zip</th>
 					<th>Next Date</th>
 					<th>Details</th>
 				</tr>

 				<tr ng-repeat="route in routes">
 					<td>{{route.name}}</td>
 					<td>{{route.tech}}</td>
 					<td>{{route.scheduleDate | date : 'MM-dd-yyyy'}}</td>
 					<td>
 						<a href="#/routes/{{route._id}}">
						<button>route</button></td>
						</a>
 				</tr>

 			</table>
 		</div>
    	</div>
 <div>
 </script>